<math xmlns="http://www.w3.org/1998/Math/MathML" alttext="{}^{{a}}_{{b}}X^{{c}}_{{d}}">
    <mmultiscripts>


       <mi>X</mi>      <!-- base expression -->

       <mi>d</mi>      <!-- postsubscript -->
       <mi>c</mi>      <!-- postsuperscript -->

       <mprescripts />
       <mi>b</mi>      <!-- presubscript -->
       <mi>a</mi>      <!-- presuperscript -->

    </mmultiscripts>
</math>